<%-include("header");-%>
    <div class="box" id="heading">
      <h1><%=ListTitle%></h1>
    </div>
    <div class="box">
      <%for(var i=0;i<listitems.length;i++){ %>
        <div class="item">
          <input type="checkbox">
          <p><%=listitems[i]%></p>
        </div>
      <% } %>
      <form class="item" action="/" method="post">
        <input type="text" name="listitem" value="" placeholder="new item" autocomplete="off">
        <button type="submit" name="List" value=<%=ListTitle%>>+</button>
      </form>
    </div>

<%-include("footer");-%>
